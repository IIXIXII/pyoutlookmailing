{%- if link is defined %}
    <a href="{{link.url}}" 
        {% if link.title is not none -%} title="{{link.title}}" {% endif -%}
        {% if toggle is defined -%} data-toggle="{{toggle}}" {% endif -%}
    >{{link.name}}</a>
{% endif -%}
